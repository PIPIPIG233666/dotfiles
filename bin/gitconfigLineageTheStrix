#!/bin/bash

USERNAME=TheStrix
EMAIL=parthbhatia@lineageos.org

echo "Setting git configs for name $USERNAME with gmail email: $EMAIL"
git config --global user.name "$USERNAME"
git config --global user.email "$EMAIL"
echo "Setting lineageOS gerrit username $USERNAME and email $EMAIL"
git config --global review.review.lineageos.org.username "$USERNAME"
git config --global review.review.lineageos.org.email "$EMAIL"
echo "Done!"
